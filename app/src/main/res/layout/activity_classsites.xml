<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.strobertchs.finalproject.classsites">

    <TextView
        android:id="@+id/mathDpnt"
        android:layout_width="252dp"
        android:layout_height="57dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="16dp"
        android:autoLink="web"
        android:text="https://sites.google.com/ycdsbk12.ca/strobertmath"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.146"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/physedDpnt"
        android:layout_width="302dp"
        android:layout_height="68dp"
        android:layout_marginBottom="52dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="88dp"
        android:autoLink="web"
        android:text="https://sites.google.com/a/ycdsb.ca/st-robert-phys-ed/home"
        android:textSize="20sp"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.257"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/gclasslink"
        android:layout_width="311dp"
        android:layout_height="58dp"
        android:layout_marginBottom="40dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:autoLink="web"
        android:text="https://classroom.google.com/u/0/h"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.14"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView3" />

    <TextView
        android:id="@+id/classsites"
        android:layout_width="279dp"
        android:layout_height="63dp"
        android:text="Classroom Sites"
        android:textSize="35sp"
        tools:layout_editor_absoluteX="53dp"
        tools:layout_editor_absoluteY="204dp" />

    <ImageView
        android:id="@+id/gclass"
        android:layout_width="72dp"
        android:layout_height="74dp"
        app:srcCompat="@drawable/gclass"
        tools:layout_editor_absoluteX="80dp"
        tools:layout_editor_absoluteY="267dp" />

    <TextView
        android:id="@+id/d2l"
        android:layout_width="354dp"
        android:layout_height="60dp"
        android:autoLink="web"
        android:text="https://ycdsb.elearningontario.ca/d2l/login"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="15dp"
        tools:layout_editor_absoluteY="350dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="119dp"
        android:layout_height="176dp"
        app:srcCompat="@drawable/d2l"
        tools:layout_editor_absoluteX="207dp"
        tools:layout_editor_absoluteY="220dp" />

</android.support.constraint.ConstraintLayout>
